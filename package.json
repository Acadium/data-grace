{
  "name": "data-grace",
  "version": "0.3.2",
  "main": "lib/index.js",
  "author": "Jamie Bertram <jhbertra@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/core-js": "^2.5.2",
    "@types/jest": "^24.0.23",
    "coveralls": "^3.0.5",
    "fast-check": "^1.16.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.7.2"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "cover": "jest --coverage",
    "docs": "typedoc",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "yarn build && tslint -c tslint.json 'src/**/*.ts'",
    "test": "yarn lint && jest"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+(?<!utilityTypes)\\.spec\\.ts$"
  },
  "dependencies": {
    "core-js": "^3.4.1"
  }
}
